{
  "name": "Go & PostgreSQL with Kafka and Redis",
  "dockerComposeFile": "docker-compose.yml",
  "service": "app",
  "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
  "customizations": {
    "vscode": {
      "extensions": [
        "golang.go"
      ]
    }
  },
  "forwardPorts": [
    5432,
    9092,
    6379
  ],
  "postCreateCommand": "go version",
  "remoteUser": "vscode"
}